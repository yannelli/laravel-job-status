{
  "name": "yannelli/laravel-job-status",
  "description": "Laravel Job Status",
  "license": "MIT",
  "authors": [
    {
      "name": "Tiger Fok",
      "email": "tiger@tiger-workshop.com"
    }
  ],
  "keywords": [
    "laravel",
    "lumen",
    "job",
    "queue"
  ],
  "require": {
    "php": "^8.3",
    "illuminate/contracts": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/queue": "^12.0",
    "illuminate/support": "^12.0",
    "nesbot/carbon": "^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^9.0",
    "laravel/pint": "^1.18"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yannelli\\TrackJobStatus\\LaravelJobStatusServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Yannelli\\TrackJobStatus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yannelli\\TrackJobStatus\\Tests\\": "tests",
      "Yannelli\\TrackJobStatus\\Tests\\Data\\": "tests/Data"
    }
  },
  "scripts": {
    "lint": "vendor/bin/pint",
    "test": "vendor/bin/phpunit",
    "test:coverage": "vendor/bin/phpunit --coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
